	Doi = {10.1111/jon.12297},
	Journal = {J Neuroimaging},
	Journal-Full = {Journal of neuroimaging : official journal of the American Society of Neuroimaging},
	Keywords = {Big data; FreeSurfer; MRI; cortical thickness; entorhinal cortex; lifespan},
	Month = {May},
	Number = {3},
	Pages = {278-82},
