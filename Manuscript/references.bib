	Pmid = {26489876},
	Pst = {aheadofprint},
	Title = {Effect of socioeconomic status (SES) disparity on neural development in female African-American infants at age 1 month},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/desc.12344}}
	
