	Pmc = {PMC2823935},
	Pmid = {20005963},
	Pst = {ppublish},
	Title = {Bias in estimation of hippocampal atrophy using deformation-based morphometry arises from asymmetric global normalization: an illustration in ADNI 3 T MRI data},
	Volume = {50},
